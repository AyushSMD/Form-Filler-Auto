{
  "manifest_version": 3,
  "name": "Google Forms AutoFiller",
  "version": "1.1",
  "description": "Autofills Google Forms from a JSON file.",
  "permissions": [
    "storage",
    "scripting",
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://docs.google.com/forms/*"],
      "js": ["content.js"]
    }
  ]
}
